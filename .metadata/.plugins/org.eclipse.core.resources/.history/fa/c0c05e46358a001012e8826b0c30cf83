package Ch03;

class Box <A,B>{
	A ob1;
	B ob2;
	Box(A ob1, B ob2){
		this.ob1 = ob1;
		this.ob2 = ob2;
	}
	@Override
	public String toString() {
		return "Box [ob1=" + ob1 + ", ob2=" + ob2 + "]";
	}
	
}

class Student{
	String name;
	String major;
	Integer level;
	
	public Student(String name, String major, Integer level) {
		super();
		this.name = name;
		this.major = major;
		this.level = level;
	}

	@Override
	public String toString() {
		return "Student [name=" + name + ", major=" + major + ", level=" + level + "]";
	}
	
	
}

public class C06Ex {

	public static void main(String[] args) {
		// BOX
		Box<Integer,Integer> ob1 = new Box(10, 20);
		System.out.println(ob1);
		
		// BOX
		Box<String,String> ob2 = new Box("샤코", "코르키");
		System.out.println(ob2);
		
		// BOX
		Box<Student, Student> ob3 = new Box(new Student("아구몬","데이터",1), new Student("파닥몬","백신",2));
		System.out.println(ob3)
		;

	}

}
