package Ch01;

class Person{
	private String name;
	private int age;
	
	// 모든 인자 생성자
	public Person(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	
	// 투스트링
	@Override
	public String toString() {
		return "Person [name=" + name + ", age=" + age + "]";
	}
	
	
}

public class C10Ex {
	
	public static void func1(int num) { // 파라미터 : Primitive(데이터값 저장)
		System.out.println(num);
	}
	public static void func2(Object obj) { // 파라미터 : Reference(주소값 저장)
		System.out.println(obj);
	}
	public static Object func3(String name, int age) {
		System.out.println(name, age);
		return new Person(name,age);
	}

	public static void main(String[] args) {
		func1(10);
		func2(new Person("파워드라몬",10));
		Object ob = func3("피노키몬",20);
	}

}
